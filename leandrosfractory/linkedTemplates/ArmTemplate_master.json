{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "leandrosfractory"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureDatabricksDeltaLake1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricksDeltaLake1'"
		},
		"SnowflakeExample_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SnowflakeExample'"
		},
		"leandrotest_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'leandrotest'"
		},
		"AzureBlobStorage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorage'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://leandroKeyvault.vault.azure.net/"
		},
		"AzureKeyVault2_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://leandroKeyvault.vault.azure.net/"
		},
		"storagekey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://leandroKeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "leandrosfractory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureDatabricksDeltaLake1_accessToken": {
						"value": "[parameters('AzureDatabricksDeltaLake1_accessToken')]"
					},
					"SnowflakeExample_connectionString": {
						"value": "[parameters('SnowflakeExample_connectionString')]"
					},
					"leandrotest_connectionString": {
						"value": "[parameters('leandrotest_connectionString')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureKeyVault2_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault2_properties_typeProperties_baseUrl')]"
					},
					"storagekey_properties_typeProperties_baseUrl": {
						"value": "[parameters('storagekey_properties_typeProperties_baseUrl')]"
					},
					"AzureBlobStorage_sasUri": {
						"value": "[parameters('AzureBlobStorage_sasUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}